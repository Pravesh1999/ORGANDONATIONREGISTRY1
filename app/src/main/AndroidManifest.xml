    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login"/>
        <activity android:name=".DonorRegistry"
            android:parentActivityName=".Login"></activity>
        <activity android:name=".FactsFigures"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".HospitalLogin"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Hospital"
            android:parentActivityName=".HospitalLogin"></activity>
        <activity android:name=".FAQ"></activity>
        <activity android:name=".HospitalOrder"
            android:parentActivityName=".HospitalLogin"></activity>
        <activity android:name=".DonorDetails">
        </activity>
        <activity android:name=".NotifyOPO"></activity>
        <activity android:name=".HospitalOrder1"></activity>
        <activity android:name=".AboutUs"></activity>
        <activity android:name=".ContactUs"></activity>
        <activity android:name=".FinalRegistry"></activity>
        <activity android:name=".HospitalView"></activity>

    </application>

</manifest>